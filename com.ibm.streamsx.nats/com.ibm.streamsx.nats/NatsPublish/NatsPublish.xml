<?xml version="1.0" ?>
<operatorModel
  xmlns="http://www.ibm.com/xmlns/prod/streams/spl/operator"
  xmlns:cmn="http://www.ibm.com/xmlns/prod/streams/spl/common"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.ibm.com/xmlns/prod/streams/spl/operator operatorModel.xsd">
  <cppOperatorModel>
    <context>
      <libraryDependencies>
        <library>
          <cmn:description>nats</cmn:description>
          <cmn:managedLibrary>
            <cmn:lib>nats</cmn:lib>
            <cmn:libPath>../../lib</cmn:libPath>
            <cmn:includePath>../../include</cmn:includePath>
          </cmn:managedLibrary>
        </library>
      </libraryDependencies>
      <providesSingleThreadedContext>Never</providesSingleThreadedContext>
    </context>
    <parameters>
      <allowAny>false</allowAny>
      <parameter>
          <name>content</name>
          <description>The content of the message to write to NATS</description>
          <optional>false</optional>
          <rewriteAllowed>true</rewriteAllowed>
          <expressionMode>Expression</expressionMode>
          <type>blob</type>
          <cardinality>1</cardinality>
      </parameter>
      <parameter>
          <name>hostname</name>
          <description>The host address of the NATS server</description>
          <optional>false</optional>
          <rewriteAllowed>true</rewriteAllowed>
          <expressionMode>AttributeFree</expressionMode>
          <type>rstring</type>
          <cardinality>1</cardinality>
      </parameter>
      <parameter>
          <name>port</name>
          <description>The port of the NATS server</description>
          <optional>false</optional>
          <rewriteAllowed>true</rewriteAllowed>
          <expressionMode>AttributeFree</expressionMode>
          <type>int32</type>
          <cardinality>1</cardinality>
      </parameter>
      <parameter>
          <name>subject</name>
          <description>The subject or topic string or wildcard to subscribe to</description>
          <optional>false</optional>
          <rewriteAllowed>true</rewriteAllowed>
          <expressionMode>AttributeFree</expressionMode>
          <type>rstring</type>
          <cardinality>1</cardinality>
      </parameter>
      <parameter>
          <name>trustedCertificates</name>
          <description></description>
          <optional>true</optional>
          <rewriteAllowed>true</rewriteAllowed>
          <expressionMode>AttributeFree</expressionMode>
          <type>rstring</type>
          <cardinality>1</cardinality>
      </parameter>
      <parameter>
          <name>certificateFile</name>
          <description></description>
          <optional>true</optional>
          <rewriteAllowed>true</rewriteAllowed>
          <expressionMode>AttributeFree</expressionMode>
          <type>rstring</type>
          <cardinality>1</cardinality>
      </parameter>
      <parameter>
          <name>privateKey</name>
          <description></description>
          <optional>true</optional>
          <rewriteAllowed>true</rewriteAllowed>
          <expressionMode>AttributeFree</expressionMode>
          <type>rstring</type>
          <cardinality>1</cardinality>
      </parameter>
      <parameter>
          <name>isSecure</name>
          <description></description>
          <optional>true</optional>
          <rewriteAllowed>true</rewriteAllowed>
          <expressionMode>AttributeFree</expressionMode>
          <type>boolean</type>
          <cardinality>1</cardinality>
      </parameter>
      <parameter>
          <name>skipVerification</name>
          <description></description>
          <optional>true</optional>
          <rewriteAllowed>true</rewriteAllowed>
          <expressionMode>AttributeFree</expressionMode>
          <type>boolean</type>
          <cardinality>1</cardinality>
      </parameter>
      <parameter>
          <name>allowReconnect</name>
          <description></description>
          <optional>true</optional>
          <rewriteAllowed>true</rewriteAllowed>
          <expressionMode>AttributeFree</expressionMode>
          <type>boolean</type>
          <cardinality>1</cardinality>
      </parameter>
      <parameter>
          <name>reconnectAttempts</name>
          <description></description>
          <optional>true</optional>
          <rewriteAllowed>true</rewriteAllowed>
          <expressionMode>AttributeFree</expressionMode>
          <type>int32</type>
          <cardinality>1</cardinality>
      </parameter>
      <parameter>
          <name>reconnectDelaySeconds</name>
          <description></description>
          <optional>true</optional>
          <rewriteAllowed>true</rewriteAllowed>
          <expressionMode>AttributeFree</expressionMode>
          <type>int32</type>
          <cardinality>1</cardinality>
      </parameter>
    </parameters>
    <inputPorts>
      <inputPortSet>
        <tupleMutationAllowed>false</tupleMutationAllowed>
        <windowingMode>NonWindowed</windowingMode>
        <windowPunctuationInputMode>Oblivious</windowPunctuationInputMode>
        <cardinality>1</cardinality>
        <optional>false</optional>
      </inputPortSet>
    </inputPorts>
    <outputPorts>
    </outputPorts>
  </cppOperatorModel>
</operatorModel>
